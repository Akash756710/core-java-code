package strings;

public class Example {

	public static void main(String[] args) {
//		String str="new String";
//		System.out.println(str.concat("this is"));
//		System.out.println(str.equals("new String"));
		
		String str="this is string";
		
		String[] words=str.split(" ");
		System.out.println("string divided in to word......");
		System.out.println();
		for(String w:words) {
			System.out.println(w);
		}
		

	}

}
