package object;
import java.util.Scanner;
public class Height {
public static void main(String[] args) {
	

	Scanner sc=new Scanner(System.in);
	
	System.out.println("Enter the height");
	int hight;
	System.out.println("Enter the number of stuent");
	int n=sc.nextInt();
	for(int i=0;i<=n;i++) {
		hight=sc.nextInt();
		System.out.println(hight/n);
	}
	
	
}
}
