package abstrac;

public class Square extends Rectengle{

	public Square(String color, double widht, double length) {
		super(color, widht, length);
		if(widht==length) 
			void area() {
			System.out.println("Area of the  " +(length*widht));
		}
			System.out.println("Premeter of the "+ (4*widht));
			}
			
		
	}
	

}
