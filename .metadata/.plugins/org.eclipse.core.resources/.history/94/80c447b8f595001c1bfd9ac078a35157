package object;

import java.text.ParseException;

public class MainEmp {

	public static void main(String[] args) throws ParseException {
		/*
		 * Employee em=new Employee(); em.readEmpDetails(); em.displayStudentDetails();
		 */
		
         Employee[] listOfEmployee  =new Employee[2];
         Employee employee;
         
         for(int i=0;i<listOfEmployee.length;i++)
         {
        	 employee =new Employee();
        	 employee.readEmpDetails();
        	 listOfEmployee[i] =employee;
         }
         for(int i=0;i<listOfEmployee.length;i++)
         {
        	 System.out.println(listOfEmployee);
        	 listOfEmployee[i].displayStudentDetails()
         }
	}

}
